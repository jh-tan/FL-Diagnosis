     const ans = [0,0]\r\n        for(let i of currentScore.score1){\r\n            ans[0]+=i\r\n        }\r\n        ans[0] = ans[0]/5\r\n        for(let i of currentScore.score2)\r\n            ans[1]+=i\r\n        ans[1] = ans[1]/13\r\n        return {age:(currentScore.age<45?0.50:currentScore.age<65?0.7:1.0),score1:ans[0],score2:ans[1]};\r\n    }\r\n    \r\n    useEffect(()=>{\r\n        console.log(currentScore)\r\n        quesNum === 20?axios.post('/api/result',calculateScore(currentScore))\r\n        .then(result=>setResult(result.data)).catch(error=>console.log(error)):console.log('')\r\n    },[currentScore])\r\n\r\n    \r\n    return(\r\n        <div>\r\n            {\r\n                quesNum < 20 ?\r\n                <div className=\"questionBank\">\r\n                    <h1>{question[`q${quesNum}`].question}</h1>\r\n                    <Answer Qanswer={{ setQuesNum, setScore, quesNum, currentScore, currentQuestion }} /> \r\n                </div>\r\n                :\r\n                    <Result result={{ setQuesNum, setSlider,setScore,calculatedResult,currentScore}} />\r\n\r\n            }\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Question","C:\\Users\\Jian Hui\\Desktop\\Backend\\src\\main\\frontend\\src\\Components\\Answer.js",[],"C:\\Users\\Jian Hui\\Desktop\\Backend\\src\\main\\frontend\\src\\Components\\Result.js",[],"C:\\Users\\Jian Hui\\Desktop\\Backend\\src\\main\\frontend\\src\\Components\\QuestionBank.js",[],"C:\\Users\\Jian Hui\\Desktop\\Backend\\src\\main\\frontend\\src\\Components\\questionBank.js",[],"C:\\Users\\Jian Hui\\Desktop\\Backend\\src\\main\\frontend\\src\\Components\\About.js",[],"C:\\Users\\Jian Hui\\Desktop\\Backend\\src\\main\\frontend\\src\\Components\\Home.js",[],"C:\\Users\\Jian Hui\\Desktop\\Backend\\src\\main\\frontend\\src\\Components\\Project.js",["73"],"import React from 'react'\r\nimport inference from '../img/inference.png'\r\n\r\nconst Project = ()=>{\r\n    return (\r\n        <div className=\"projectContainer\">\r\n            <h1>About This Project</h1>\r\n            <img src={inference}/>\r\n            <div className=\"projectDetail\">\r\n                <h1>What is the purpose of this system</h1>\r\n                <p>\r\n                    This system can diagnose lung cancer based on risk factor and symptoms. This\r\n                    system provides the probability of getting lung cancer. The process of diagnosing \r\n                    lung cancer cannot be carried out with incomplete information, so this\r\n                    expert systems require intelligent inference techniques to make decisions with high accuracy.\r\n                </p>\r\n                <h1>How does this system work</h1>\r\n                <p>\r\n                    Fuzzy logic is used in this expert system. Fuzzy may considered as \r\n                    forward - chaining as it is data driving inferencing process. In fuzzy inference\r\n                    process, it begin with crisp numbers constrained. For this system, the crisp value\r\n                    are age and score1 ( risk factor ), both double from 0 to 1. The system then return \r\n                    first output in defuzzified form using centroid method. Next, a crisp score between \r\n                    0 to 1 is generated using binary variables of patient's symptoms. Finally, the first\r\n                    defuzzified output and the crisp score about patient's symptoms are used to produce \r\n                    the final result of the system. Defuzzification in this step is also based on centroid \r\n                    method and the obtained value shows the probability of lung cancer.\r\n                </p>\r\n                <p>\r\n                    All rules are evaluated by apply fuzzy operator on the antecedent and also implication method. \r\n                    Implication method means weighting consequent with antecedent(a single number). \r\n                </p>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Project","C:\\Users\\Jian Hui\\Desktop\\Backend\\src\\main\\frontend\\src\\Components\\Diagnosis.js",[],"C:\\Users\\Jian Hui\\Desktop\\Backend\\src\\main\\frontend\\src\\Components\\Collapsible.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":29,"column":7,"nodeType":"80","endLine":29,"endColumn":21,"suggestions":"81"},{"ruleId":"82","severity":1,"message":"83","line":8,"column":13,"nodeType":"84","endLine":8,"endColumn":35},"no-native-reassign",["85"],"no-negated-in-lhs",["86"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'calculateScore' and 'quesNum'. Either include them or remove the dependency array.","ArrayExpression",["87"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation",{"desc":"88","fix":"89"},"Update the dependencies array to be: [calculateScore, currentScore, quesNum]",{"range":"90","text":"91"},[1086,1100],"[calculateScore, currentScore, quesNum]"]